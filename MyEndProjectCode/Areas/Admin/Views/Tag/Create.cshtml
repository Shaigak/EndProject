@using MyEndProjectCode.Areas.View_Models

@model TagCreateVM
@{
    ViewData["Title"] = "Create";
}

<div class="container my-5 ">

    <form class="forms-sample" asp-action="Create" method="post" enctype="multipart/form-data">
        

        <span>Add Tag</span>

        <input type="text" class="form-control" id="customFile" asp-for="Year" />

        <span class="text-danger" asp-validation-for="Year"></span>


        <div class="buttons my-3">

            <button type="submit" class="btn btn-primary mr-2">Submit</button>
            <a class="btn btn-light" asp-action="Index">Cancel</a>
        </div>
    </form>

</div>
